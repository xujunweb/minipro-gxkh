
var DateFr = {
  add0:function(m){
    return m < 10 ? '0' + m : m
  },
  getDate: function (shijianchuo) {
    console.log('dfdfdsfsdfsdfs', shijianchuo)
    //shijianchuo是整数，否则要parseInt转换
    var time = getDate(shijianchuo);
    console.log('dfdfdsfsdfsdfs', time)
