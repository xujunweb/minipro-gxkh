/* pages/my/agent/agent.wxss */
.caidan{
  padding-left: 40rpx;
  margin-bottom: 50rpx;
}
.caidan .item{
  padding: 20rpx 40rpx;
  padding-left: 0;
  border-bottom: 2rpx solid #e6e6e6;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}
.caidan .item .left{
  display: flex;align-items: center;
}
.caidan .item .arrwo{
  width: 20rpx;
  height: 20rpx;
  border-top: 2rpx solid #ccc;
  border-right: 2rpx solid #ccc;
  transform: rotate(45deg);
}
.caidan .item .text{
  color: #999;font-size: 26rpx;
}
.caidan .item image{
  width: 50rpx;height: 50rpx;margin-right: 20rpx;
}
