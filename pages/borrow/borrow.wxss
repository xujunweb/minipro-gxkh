.log-list{
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: #F5F5F5;
  padding: 0 60rpx;
}
.log-list .content{
  background: #fff;
  border-radius: 30rpx;
}
.log-list image{
  width: 100%;
}
.text_line{
  font-size: 28rpx;
  text-align: center;
}
.other-button-hover {
  background-color: blue;
}
.image{
  text-align: center;
}
.button{
  color: #fff;
  margin-top: 70rpx;
  margin-bottom: 70rpx;
  width: 85%;
  background:#E23364;
  border-radius:40rpx;
  height: 88rpx;
  line-height: 88rpx;
  box-shadow: 0 0 20rpx #c1bbbb;
}