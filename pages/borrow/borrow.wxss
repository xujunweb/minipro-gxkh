.log-list{
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: #F5F5F5;
}
.log-list .content{
  width: 678rpx;
  margin: 0 auto;
  border-radius: 30rpx;
  padding-bottom: 20rpx;
}
.h2-title{
  color: #E23364;font-size: 40rpx;padding-left:61rpx;
  font-weight: 500;line-height: 60rpx;margin-bottom: 50rpx;
}
.log-list image{
  width: 100%;
}
.text_line{
  font-size: 28rpx;
  text-align: center;
}
.other-button-hover {
  background-color: blue;
}
.image{
  text-align: center;
}
.button{
  color: #fff;
  margin-top: 70rpx;
  width: 85%;
  background:#E23364;
  border-radius:40rpx;
  height: 88rpx;
  line-height: 88rpx;
  box-shadow: 0 0 20rpx #c1bbbb;
}
.text{
  font-size: 30rpx;color: #999;margin-top: 30rpx;text-align: center;
}
.swiper{
  width: 630rpx;height: 480rpx;margin: 0 auto;background: #999;
  border-radius: 10rpx;overflow: hidden;box-shadow: 0 0 20rpx #999;
}
.swiper swiper{
  width: 630rpx;height: 480rpx;overflow: hidden;
}
.swiper swiper-item{
  border-radius: 10rpx;overflow: hidden;
}
.swiper image{
  /* border-radius: 20rpx; */
  width: 630rpx;height: 480rpx;
}

/* 短信验证码登录 */
.login_view{
  position: fixed; z-index: 9; top: 0; left: 0; width: 100%; height: 100%;
}
.layer_bg {
  position: absolute; z-index: 9; top: 0; left: 0; width: 100%; height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
}
.login_view .content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-content: center;
  align-items: center;
}
.login_view .content .login_main {
  position: relative;z-index: 99;
  width: 600rpx; height: auto;
  background-color: #fff; border-radius: 10rpx;
  padding: 0rpx;margin-bottom: 20rpx;
  overflow: hidden;
}
.lit_title{
  font-size: 24rpx;color: #999;margin-top: 20rpx;padding: 0 85rpx;
}
.login_view .content .login_main .upperPart{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-flow: column;
  padding: 100rpx 0;
}
.login_view .content .login_main .upperPart image{
  width: 300rpx;height: 130rpx;
}
.login_view .content .login_main .lowerPart{
  display: flex;flex-flow: column;justify-content: center;
  align-items: center;padding: 70rpx 0;
  background: #fafafa;text-align: center;
}
.login_view .content .login_main .lowerPart .button{
  margin-top: 0rpx;font-size: 30rpx;
}
.login_title {
  text-align: center;font-size: 36rpx;line-height: 45rpx;color: #333;
  margin-top: 20rpx;font-weight: 500;
}
.code .code_def{ 
  width: 280rpx; height: 88rpx; line-height: 88rpx; 
  color: #d3a95a; font-size:26rpx;
  text-align: right; margin-right: 30rpx;
}
.code .code_sel { color: #888; }
.logout_btn {
  width: 60rpx; height: 60rpx; z-index: 999;
}
.logout_btn image{
  width: 60rpx; height: 60rpx;
}